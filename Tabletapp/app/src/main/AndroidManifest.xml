<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.test1.tabletapp.app" >
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_INTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <uses-sdk android:minSdkVersion="11" />

    <application

        android:allowBackup="true"
        android:icon="@drawable/pic"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="company.Start"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="company.Entry_show"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="company.SmplExpandable"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="company.New_Entry_Select"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="company.Entry_Details"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="company.Project_entries_show"
            android:label="@string/app_name">
            </activity>
        <activity android:name="company.Project_Details"
            android:label="Tabletapp">
            </activity>
        <activity android:name="company.Experiment_Details"
            android:label="Tabletapp">
            </activity>

        <activity
            android:name="company.Keyboard_entry"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="company.Table_entry"
            android:label="@string/app_name" >
        </activity>

        <service
            android:name="company.LocalService"
            android:enabled="true"
            android:exported="false"
            android:permission="">
        <intent-filter>
                <action android:name="company.LocalService">
                </action>
            </intent-filter>
        </service>



        <activity
            android:name="company.Project_show"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>
